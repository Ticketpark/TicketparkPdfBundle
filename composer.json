{
    "name": "ticketpark/pdf-bundle",
    "type": "symfony-bundle",
    "description": "This bundle assists in creating PDF files with DocRaptor",
    "keywords": ["pdf"],
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Ticketpark contributors and others",
            "homepage": "https://github.com/Ticketpark/PdfBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "bytes/docraptor-bundle": "dev-master",
        "bytes/docraptor": "dev-master",
        "ticketpark/file-bundle": "~0.2"
    },

    "autoload": {
        "psr-0": { "Ticketpark\\PdfBundle": "" }
    },

    "target-dir": "Ticketpark/PdfBundle"
}
